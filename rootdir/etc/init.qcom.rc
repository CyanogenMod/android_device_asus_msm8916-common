    chown system system /sys/bus/iio/devices/iio:device0/power
    chmod 774 /sys/bus/iio/devices/iio:device0/power

    chown system.system /sys/bus/iio/devices/iio:device0/*
    chmod 774 /sys/bus/iio/devices/iio:device0/*
