    mkdir /data/nfc/param 0770 nfc nfc

    # Permissions for touchscreen
    chown system system /sys/bus/i2c/devices/i2c-5/5-0038/glove_mode
    chmod 0664 /sys/bus/i2c/devices/i2c-5/5-0038/glove_mode
